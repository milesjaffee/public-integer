generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model IntegerValue {
  id         Int      @id @default(1)
  value      Int
  updated_at DateTime @default(now())
}

model IntegerHistory {
  id         Int      @id @default(autoincrement())
  value      Int
  updated_at DateTime @default(now())
}
